{
  "editor.tabSize": 2,
  "vim.neovimPath": "/opt/homebrew/bin/nvim",
  "editor.multiCursorModifier": "ctrlCmd",
  "keyboard.dispatch": "keyCode",
  "emmet.showSuggestionsAsSnippets": true,
  "editor.suggestSelection": "first",
  "html.format.indentHandlebars": true,
  "emmet.excludeLanguages": ["markdown"],
  "vim.easymotion": true,
  "vim.sneak": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.minimap.enabled": false,
  "editor.quickSuggestionsDelay": 12,
  "vim.neovimUseConfigFile": true,
  "git.ignoreMissingGitWarning": true,
  "editor.accessibilitySupport": "off",
  "debug.onTaskErrors": "debugAnyway",
  "editor.fontSize": 17,
  "editor.accessibilityPageSize": 17,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["<leader>", "<leader>", "s"],
      "before": ["s"]
    }
  ],
  "liveServer.settings.donotShowInfoMsg": true,
  "json.schemas": [],
  "editor.bracketPairColorization.enabled": true,
  "vim.enableNeovim": true,
  "window.zoomLevel": 2,
  "vim.smartRelativeLine": true,
  "explorer.openEditors.visible": 8,
  "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/.project": true,
    "**/.settings": true,
    "**/.classpath": true,
    "**/.factorypath": true
  },
  "[dart]": {
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off",
    "editor.selectionHighlight": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80]
  },
  "html.format.indentInnerHtml": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "breadcrumbs.enabled": false,
  "terminal.integrated.allowChords": false,
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.suggest.showMethods": true,
  "editor.suggest.preview": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.snippetSuggestions": "top",
  "vim.vimrc.enable": true,
  "redhat.telemetry.enabled": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.startupEditor": "none",
  "git.openRepositoryInParentFolders": "never",
  "editor.guides.bracketPairs": true,
  "terminal.integrated.fontSize": 18,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "One Dark italic",
        "scope": [
          "comment",
          "entity.other.attribute-name",
          "keyword",
          "markup.underline.link",
          "storage.modifier",
          "storage.type",
          "string.url",
          "variable.language.super",
          "variable.language.this"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "One Dark italic reset",
        "scope": [
          "keyword.operator",
          "keyword.other.type",
          "storage.modifier.import",
          "storage.modifier.package",
          "storage.type.built-in",
          "storage.type.function.arrow",
          "storage.type.generic",
          "storage.type.java",
          "storage.type.primitive"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "RainbowBrackets.depreciation-notice": false,
  "files.autoSave": "afterDelay",
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.applyToAllProfiles": ["maven.executable.path"],
  "diffEditor.renderSideBySide": false,
  "workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
  "hediet.vscode-drawio.resizeImages": null,
  "mojo.onSettingsChanged": "restart",

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
